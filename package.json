{
  "name": "grafic-ecommerce",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3003",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3003",
    "generate-download": "bash scripts/create-download.sh",
    "task:status": "node scripts/task.js status",
    "task:start": "node scripts/task.js start",
    "task:finish": "node scripts/task.js finish",
    "sync-gelato": "node --experimental-modules scripts/sync-gelato-products.js",
    "fetch-blank": "node scripts/fetch-blank-product.js",
    "fetch-store": "node scripts/fetch-store-product.js",
    "test": "npm run test:unit",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest --watch",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:update": "playwright test --update-snapshots",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.57.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.15.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.469.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-helmet": "^6.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.22.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.5",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.169.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "jsdom": "^24.1.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "vite": "^4.4.5",
    "vitest": "^1.6.0"
  }
}
